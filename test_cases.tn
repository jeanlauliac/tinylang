export u32 (vec<str> args) {
  vec<str> stuff = ["Hello"];
  add_world(&stuff);
  IO.print(stuff[0] + stuff[1]);
  return 0;
}
void add_world(vec<str> &stuff) {
  push(&stuff, ", World");
}
####
{
  "output": "Hello, World\n"
}
///////////////////////////////////////////////////////////////////////////////
export u32 (vec<str> args) {
  vec<str> stuff = ["Hello"];
  add_world(stuff);
  push(&stuff, ", you");
  IO.print(stuff[0] + stuff[1]);
  return 0;
}
void add_world(vec<str> stuff) {
  push(&stuff, ", World");
}
####
{
  "output": "Hello, you\n"
}
